{
  "name": "angular-starter",
  "version": "0.0.1",
  "description": "angular starter",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && webpack --config ./config/webpack.build.config.js --progress",
    "dev": "webpack-dev-server --config ./config/webpack.dev.config.js --progress --content-base ./src",
    "server": "http-server dist -p 3000 -c-1 --cors",
    "test": "karma start ./config/karma.conf.js",
    "e2e": "npm-run-all -p -r server protractor:delay",
    "lint": "tslint ./src/**/*.ts",
    "clean": "rimraf dist",
    "postinstall": "npm run webdriver:update",
    "protractor:delay": "sleep 3 && protractor ./config/protractor.conf.js",
    "webdriver:start": "webdriver-manager start",
    "webdriver:update": "webdriver-manager update"
  },
  "keywords": [
    "angular",
    "webpack",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@angular/common": "^4.0.1",
    "@angular/compiler": "^4.0.1",
    "@angular/core": "^4.0.1",
    "@angular/forms": "^4.0.1",
    "@angular/http": "^4.0.1",
    "@angular/platform-browser": "^4.0.1",
    "@angular/platform-browser-dynamic": "^4.0.1",
    "@angular/router": "^4.0.1",
    "core-js": "^2.4.1",
    "rxjs": "^5.3.0",
    "zone.js": "^0.8.5"
  },
  "devDependencies": {
    "@types/jasmine": "^2.5.47",
    "@types/node": "^7.0.12",
    "@types/webpack": "^2.2.14",
    "angular2-template-loader": "^0.6.2",
    "assets-webpack-plugin": "^3.5.1",
    "awesome-typescript-loader": "^3.1.2",
    "css-loader": "^0.28.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "http-server": "^0.9.0",
    "istanbul-instrumenter-loader": "^2.0.0",
    "jasmine-core": "^2.5.2",
    "karma": "^1.6.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.3",
    "ng-router-loader": "^2.1.0",
    "node-sass": "^4.5.2",
    "npm-run-all": "^4.0.2",
    "protractor": "^5.1.1",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.3",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.16.1",
    "to-string-loader": "^1.1.5",
    "ts-node": "^3.0.2",
    "tslint": "^4.5.1",
    "tslint-loader": "^3.4.3",
    "typescript": "^2.2.2",
    "webpack": "^2.3.3",
    "webpack-dev-server": "^2.4.2",
    "webpack-merge": "^4.1.0"
  }
}